          "prov:type": {
            "$": "var:paperType",
            "type": "prov:QUALIFIED_NAME"
          },
          "tmpl:label": {
            "$": "var:title",
            "type": "prov:QUALIFIED_NAME"
          }
        }
      },
      "prefix": {
        "pic": "http://www.ipaw.info/ns/picaso#",
        "tmpl": "http://openprovenance.org/tmpl#",
        "var": "http://openprovenance.org/var#",
        "vargen": "http://openprovenance.org/vargen#"
      },
      "wasAttributedTo": {
        "_:id1": {
          "prov:agent": "var:author",
          "prov:entity": "var:paper",
